{
  "presets": ["es2015", "stage-2"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "App": "./src"
        }
      }
    ]
  ]
}
